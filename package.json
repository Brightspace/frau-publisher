    "url": "https://github.com/Brightspace/frau-publisher/issues"
  },
  "homepage": "https://github.com/Brightspace/frau-publisher",
  "devDependencies": {
    "chai": "^4",
    "coveralls": "^3",
    "eslint": "^7",
    "eslint-config-brightspace": "^0.11",
    "gulp": "^4",
    "mocha": "^7",
    "nyc": "^15",
    "proxyquire": "^2",
    "request": "^2",
    "sinon": "^9",
    "sinon-chai": "^3",
    "source-map-support": "^0.5",
    "through2": "^4"
  },
  "dependencies": {
    "aws-sdk": "^2",
    "chalk": "^4",
    "mime-types": "^2",
