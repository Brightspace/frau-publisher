{
  "name": "frau-publisher",
  "version": "2.8.2",
  "description": "A free-range-app utility for publishing to our CDN.",
  "main": "src/publisher.js",
  "bin": {
    "frau-publisher": "bin/publishercli"
  },
  "files": [
    "bin",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "pretest": "npm run lint",
